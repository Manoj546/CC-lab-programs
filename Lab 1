#include<stdio.h>
#include<conio.h>
int main(){
   int a,b;
   clrscr();
   printf("Enter two numbers:  ");
   scanf("%d%d",&a,&b);                //taking two numbers as input
   if(a>b)                             //using a conditional statement
   printf("\n%d is greater than %d",a,b);
   else
   printf("\n%d is greater than %d",b,a);
   getch();
   return 0;
   }

#include<stdio.h>
#include<conio.h>
int main(){
   float cel,fahr;
   clrscr();
   printf("Enter a value in fahrenheit ");
   scanf("%f",&fahr);
   cel=((fahr-32)*5)/9;
   printf("\nThe value of temperature in celcius is %f",cel);
   getch();
   return 0;
}
 
